    <Copy SourceFiles="@(WebFiles)" DestinationFiles="@(WebFiles->'$(SitecorePath)\%(RelativeDir)%(Filename)%(Extension)')" SkipUnchangedFiles="true" />
  </Target>
  <Target Name="CompileLess">
    <CreateItem Include="**\*.less">
